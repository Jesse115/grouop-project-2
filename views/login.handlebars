<body>
    <div class="card">
      <h3 class="card-header">Login</h3>
      <form id="login-form" class="card-body">
        <label class="form-label" for="username-login">Username</label>
        <input  id="username-login" type="text" autofocus="true" class="form-input" />

        <label class="form-label" for="password-login">Password</label>
        <input id="password-login" type="password" autofocus="true" class="form-input" />

        <div class="form-button-layout">
        <button type="submit" class="btn"><i class="fas fa-sign-in-alt" style="padding-right:8px;"></i>Login</button>
        {{!-- <button class="btn-outline"><a class="link-blue" href="/signup"><i class="fas fa-user-plus" style="padding-right: 8px;"></i>Sign up</a></button> --}}
        </div>
      </form>
    </div>


    <div class="card">
      <h3 class="card-header">Sign Up</h3>
      <form id="signup-form" class="card-body">
        <label for="username-signup" class="form-label">Username</label>
        <input id="username-signup" type="text" class="form-input" />


        <label class="form-label" for="password-signup">Password</label>
        <input  id="password-signup" type="password" class="form-input" />

        <label class="form-label" for="email-signup">Email</label>
        <input  id="email-signup" type="email" class="form-input" />

        <div class="form-button-layout">
        <button type="submit" class="btn">Sign up</button>
        {{!-- <button class="btn-outline"><a class="link-blue" href="/login"><i class="fas fa-sign-in-alt" style="padding-right:8px;"></i>Log in</a></button> --}}
      </form>
      </div>
    </div>
    </body>

